{
  "name": "van_krevelen_wizard",
  "version": "1.0.0",
  "description": "van_krevelen_wizard",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . van_krevelen_wizard --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"van_krevelen_wizard\"",
    "package-linux": "electron-packager . van_krevelen_wizard --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Jakob Notland",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "electron": "^9.0.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "electron-packager": "^14.2.1"
  }
}
